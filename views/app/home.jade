extends ../layout.jade
block contenido
    for imagen in imagenes
        div(class="img-container")
            h2= imagen.title
            img(src="/public/images/#{imagen._id}.#{imagen.extension}", class="main-img")
            if(typeof imagen.creator != "undefined")
                p By #{imagen.creator.email}
